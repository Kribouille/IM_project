<?xml version="1.0" encoding="UTF-8"?>
<twitterRESTAPI:DeckSet
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:twitterRESTAPI="http://www.example.org/twitterRESTAPI"
    xsi:schemaLocation="http://www.example.org/twitterRESTAPI ../twitterRESTAPI.ecore">
  <deck name="myName"
      initFilters="//@deck.0/@instructions.0">
    <instructions
        xsi:type="twitterRESTAPI:User"
        unOp="&lt;"
        value="SaintePelle">
      <next
          xsi:type="twitterRESTAPI:Date"
          value="1993-05-25T00:00:00.000+0200"/>
    </instructions>
    <instructions
        xsi:type="twitterRESTAPI:Date"
        unOp="&lt;"
        value="2017-01-01T00:00:00.000+0100">
      <next
          xsi:type="twitterRESTAPI:Retweet"
          unOp=">"
          value="6">
        <next
            xsi:type="twitterRESTAPI:Word"
            value="lol"/>
      </next>
    </instructions>
    <instructions
        xsi:type="twitterRESTAPI:Hashtag"
        value="dog">
      <next
          xsi:type="twitterRESTAPI:Hashtag"
          value="pig">
        <next
            xsi:type="twitterRESTAPI:User"
            value="SaintePelle"/>
      </next>
    </instructions>
  </deck>
</twitterRESTAPI:DeckSet>
