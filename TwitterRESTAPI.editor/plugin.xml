<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated twitterRESTAPI -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="twitterRESTAPI.presentation.TwitterRESTAPIModelWizardID"
            name="%_UI_TwitterRESTAPIModelWizard_label"
            class="twitterRESTAPI.presentation.TwitterRESTAPIModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/TwitterRESTAPIModelFile.gif">
         <description>%_UI_TwitterRESTAPIModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated twitterRESTAPI -->
      <editor
            id="twitterRESTAPI.presentation.TwitterRESTAPIEditorID"
            name="%_UI_TwitterRESTAPIEditor_label"
            icon="icons/full/obj16/TwitterRESTAPIModelFile.gif"
            extensions="twitterrestapi"
            class="twitterRESTAPI.presentation.TwitterRESTAPIEditor"
            contributorClass="twitterRESTAPI.presentation.TwitterRESTAPIActionBarContributor">
      </editor>
   </extension>

</plugin>
